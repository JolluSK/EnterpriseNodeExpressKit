{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off",
        "no-unused-vars": "warn",
        "prettier/prettier": "error"
    }
}